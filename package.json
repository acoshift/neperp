{
  "name": "nepstation",
  "version": "1.0.0",
  "description": "NepStack front-end",
  "main": "",
  "scripts": {
    "clean": "rimraf node_modules doc && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe --devtool=none",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "docs": "typedoc --options typedoc.json src/**/*.ts",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acoshift/nepstation.git"
  },
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/acoshift/nepstation/issues"
  },
  "homepage": "https://github.com/acoshift/nepstation#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.0",
    "core-js": "^1.2.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.34.1",
    "es7-reflect-metadata": "^1.4.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.11.0",
    "rxjs": "^5.0.0-beta.0",
    "semantic-ui-css": "^2.1.7",
    "zone.js": "^0.5.10"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "imports-loader": "^0.6.4",
    "jade": "^1.11.0",
    "jade-html-loader": "0.0.3",
    "json-loader": "^0.5.3",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.0",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "to-string-loader": "^1.1.3",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.1",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.0-dev.20160106",
    "uglify-loader": "^1.3.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  }
}
